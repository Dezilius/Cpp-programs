void __fastcall TFrame4::Button1Click(TObject *Sender)
{
     Form3->Hide();
     Form2->Show();    
}
//---------------------------------------------------------------------------
void __fastcall TFrame4::Button2Click(TObject *Sender)
{
     Form3->Hide();
     Form1->Show();    
}
